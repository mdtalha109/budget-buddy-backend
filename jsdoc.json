{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs)"
    },
    "opts": {
      "destination": "./docs/",
      "recurse": true,
      "readme": "./README.md"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }